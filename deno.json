  "tasks": {
    "run": "deno run --allow-net --allow-env=NODE_V8_COVERAGE,UNDICI_NO_FG,JEST_WORKER_ID,GOOGLE_API_KEY,LANGCHAIN_VERBOSE,LANGCHAIN_TRACING,LANGCHAIN_TRACING_V2,LANGSMITH_TRACING,LANGSMITH_TRACING_V2 src/cli.ts"
